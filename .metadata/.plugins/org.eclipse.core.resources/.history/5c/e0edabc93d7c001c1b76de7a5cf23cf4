
		// Assignment: 1. Rotate the Image to Right -> 90 degrees
	 	//			   2. Rotate the Image to Left  -> 180 degrees
	 	//			   3. GrayScale the image
	 	//					for a pixel sum up all values and divide by 3
	 	//				    eg: for pixel1 -> 12+45+78/3 -> 45
	 	//					pixel1 = {45, 45, 45}
	 	//					do it for all pixels on the image
	 	// 			   Hint: Use For Loops :)
public class ImageRotate {

	public static void main(String[] args) {
		int[] pixel1 = {12, 45, 78};
	 	int[] pixel2 = {34, 12, 90};
	 	int[] pixel3 = {0, 234, 56};
	 	
	 	int[] pixel4 = {13, 46, 79};
	 	int[] pixel5 = {22, 35, 68};
	 	int[] pixel6 = {2, 55, 88};
	 	
	 	int[] pixel7 = {42, 95, 68};
	 	int[] pixel8 = {52, 55, 98};
	 	int[] pixel9 = {22, 85, 48};
	 	
	 	// Representation of an Image
	 	int[][][] image = {
	 						{pixel1, pixel2, pixel3},
	 						{pixel4, pixel5, pixel6},
	 						{pixel7, pixel8, pixel9},
	 					};
	 		 	

	 	for(int i=0;i<3;i++) {
	 		for(int j=0;j<3;j++){
	 			System.out.println(image[i][j]);
	 		}
	 		System.out.println();
	 	}
	 	
	 	for(int i=0;i<3;i++) {
	 		for(int j=0;j<3;j++){
	 			for(int k=0;k<3;k++){
	 				System.out.print(image[i][j][k]+" ");
	 			}
	 			System.out.println();
	 		}
	 	}
	}

}
