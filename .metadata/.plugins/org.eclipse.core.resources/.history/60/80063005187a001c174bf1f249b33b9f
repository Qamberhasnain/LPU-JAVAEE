
public class BreakAndContinue {

	public static void main(String[] args) {
		Label1:
		      for(int i=0; i<10; i++){
		         if (i==5){
		            break Label1;
		          }
		          System.out.println("......."+i );
		      }
		Label2:
		      for(int i=0; i<10; i++){
		         if (i==5){
		            continue Label2;
		          }
		          System.out.println("......."+i );
		      }
	}

}
